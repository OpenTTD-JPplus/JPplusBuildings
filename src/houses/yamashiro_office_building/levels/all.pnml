
// yamashiro_office_building levels

// yamashiro_office_building all colours

// _grey_
    // Norm Sprites
    spritelayout sprlay_yamashiro_office_building_north_grey_norm {
        ground {
            sprite: spr_yamashiro_office_building_north_ground (construction_state);
        }
        building {
            sprite: spr_yamashiro_office_building_north_norm (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 1;
            xoffset: 2;
            yoffset: 2;
        }
    }

    // Snow Sprites
    spritelayout sprlay_yamashiro_office_building_north_grey_snow {
        ground {
            sprite: spr_yamashiro_office_building_north_ground_snow (construction_state);
        }
        building {
            sprite: spr_yamashiro_office_building_north_snow (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 1;
            xoffset: 2;
            yoffset: 2;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_yamashiro_office_building_north_grey_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_yamashiro_office_building_north_grey_snow;
	    sprlay_yamashiro_office_building_north_grey_norm;
    }
