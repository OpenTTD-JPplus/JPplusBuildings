
// hashimoto_office_tower all colours

// _white_
    // Norm Sprites
    spritelayout sprlay_hashimoto_office_tower_xL_white_norm {
        ground {
            sprite: spr_hashimoto_office_tower_ground (3);
        }
        building {
            sprite: spr_hashimoto_office_tower_xL_norm (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_hashimoto_office_tower_xL_white_snow {
        ground {
            sprite: spr_hashimoto_office_tower_ground_snow (3);
        }
        building {
            sprite: spr_hashimoto_office_tower_xL_snow (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_hashimoto_office_tower_xL_white_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_hashimoto_office_tower_xL_white_snow;
	    sprlay_hashimoto_office_tower_xL_white_norm;
    }
