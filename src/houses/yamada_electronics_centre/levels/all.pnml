    spritelayout sprlay_yamada_electronics_centre_8L_white_norm {
        ground {
            sprite: spr_yamada_electronics_centre_ground (construction_state);
        }
        building {
            sprite: spr_yamada_electronics_centre_8L_norm (construction_state);
