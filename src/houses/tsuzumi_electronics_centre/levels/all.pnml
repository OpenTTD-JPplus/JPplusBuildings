
// tsuzumi_electronics_centre levels

// _red_brown_
    // Norm Sprites
    spritelayout sprlay_tsuzumi_electronics_centre_a_p_red_brown_norm {
        ground {
            sprite: spr_tsuzumi_electronics_centre_a_ground (construction_state);
        }
        building {
            sprite: spr_tsuzumi_electronics_centre_a_p_norm (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 13;
            xoffset: 2;
        }
    }

    // Snow Sprites
    spritelayout sprlay_tsuzumi_electronics_centre_a_p_red_brown_snow {
        ground {
            sprite: spr_tsuzumi_electronics_centre_a_ground_snow (construction_state);
        }
        building {
            sprite: spr_tsuzumi_electronics_centre_a_p_snow (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 13;
            xoffset: 2;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_tsuzumi_electronics_centre_a_p_red_brown_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_tsuzumi_electronics_centre_a_p_red_brown_snow;
	    sprlay_tsuzumi_electronics_centre_a_p_red_brown_norm;
    }

