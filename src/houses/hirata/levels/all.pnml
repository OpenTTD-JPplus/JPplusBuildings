// _white_
    // Norm Sprites
    spritelayout sprlay_hirata_a_3L_white_norm {
        ground {
            sprite: spr_hirata_a_ground (construction_state);
        }
        building {
            sprite: spr_hirata_a_3L_norm (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

