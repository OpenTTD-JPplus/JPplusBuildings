
// yamaguchi_office all colours

// _white_
    // Norm Sprites
    spritelayout sprlay_yamaguchi_office_xL_white_norm {
        ground {
            sprite: spr_yamaguchi_office_ground (3);
        }
        building {
            sprite: spr_yamaguchi_office_xL_norm (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_yamaguchi_office_xL_white_snow {
        ground {
            sprite: spr_yamaguchi_office_ground_snow (3);
        }
        building {
            sprite: spr_yamaguchi_office_xL_snow (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_yamaguchi_office_xL_white_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_yamaguchi_office_xL_white_snow;
	    sprlay_yamaguchi_office_xL_white_norm;
    }
