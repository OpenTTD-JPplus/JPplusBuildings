
// ishida_commercial_building all colours

// _white_
    // Norm Sprites
    spritelayout sprlay_ishida_commercial_building_xL_white_norm {
        ground {
            sprite: spr_ishida_commercial_building_ground (3);
        }
        building {
            sprite: spr_ishida_commercial_building_xL_norm (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_ishida_commercial_building_xL_white_snow {
        ground {
            sprite: spr_ishida_commercial_building_ground_snow (3);
        }
        building {
            sprite: spr_ishida_commercial_building_xL_snow (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_ishida_commercial_building_xL_white_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_ishida_commercial_building_xL_white_snow;
	    sprlay_ishida_commercial_building_xL_white_norm;
    }
