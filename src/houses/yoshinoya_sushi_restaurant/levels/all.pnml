
// yoshinoya_sushi_restaurant levels

// yoshinoya_sushi_restaurant all colours

// _white_
    // Norm Sprites
    spritelayout sprlay_yoshinoya_sushi_restaurant_a_white_norm {
        ground {
            sprite: spr_ground_concrete (3);
        }
        building {
            sprite: spr_yoshinoya_sushi_restaurant_a_norm (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_yoshinoya_sushi_restaurant_a_white_snow {
        ground {
            sprite: spr_ground_concrete (3);
        }
        building {
            sprite: spr_yoshinoya_sushi_restaurant_a_snow (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_yoshinoya_sushi_restaurant_a_white_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_yoshinoya_sushi_restaurant_a_white_snow;
	    sprlay_yoshinoya_sushi_restaurant_a_white_norm;
    }
