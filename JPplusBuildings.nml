			( nearby_tile_class( 4, 1) == TILE_CLASS_HOUSE || nearby_tile_class( 4, 1) == TILE_CLASS_INDUSTRY || nearby_tile_class( 4, 1) == TILE_CLASS_OBJECTS)	
		)

