			( nearby_tile_class(-1,-2) == TILE_CLASS_ROAD  || nearby_tile_class(-1,-2) == TILE_CLASS_STATION ) &&
			( nearby_tile_class(-1,-3) == TILE_CLASS_HOUSE || nearby_tile_class(-1,-3) == TILE_CLASS_INDUSTRY || nearby_tile_class(-1,-3) == TILE_CLASS_OBJECTS)	
		)
	)
	) {return;}

